{
  "name": "roll-right",
  "version": "0.1.4",
  "description": "A utilty for generating template files, e.g. HTML templates, in one phase, and then generating final output files, e.g. HTML, in another phase.",
  "main": "index.js",
  "directories": {
    "example": "examples"
  },
  "bin": {
    "roll-right": "./bin/index.js",
    "roll-right-breakup": "./bin/breakup.js"
  },
  "scripts": {
    "test": "node tests/index.js",
    "prepare" : "node bin/index.js --phase prepare --sources ../websites/template-configs/  --generator generate.json --structure parsed.json",
    "templates" : "node bin/index.js --phase template --sources ../websites/template-configs/ --generator generate.json --structure parsed.json",
    "assignments" : "node bin/index.js --phase assign --sources ../websites/template-configs/ --values assignments.json"
  },
  "dependencies": {
    "byte-base64": "^1.1.0",
    "extra-file-class": "^1.0.1",
    "handlebars": "^4.7.7",
    "minimist": "^1.2.8",
    "untildify": "^4.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/copious-world/roll-right.git"
  },
  "keywords": [
    "browserify",
    "rollup",
    "publisher",
    "HTML-generation"
  ],
  "author": "R. Leddy",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/copious-world/roll-right/issues"
  },
  "homepage": "https://github.com/copious-world/roll-right#readme"
}
